{
	"manifest_version" : 2,
	"name" : "Address saver",
	"version" : "1.0",
	"description" : " Saves the address in the form of link",
	"icons": {
		"16": "manutd.png",
		"48": "manutd4848.png",
		"128" : "manutd128.png"
	},
	"permissions" : ["storage"],
	"background": {
		"scripts" : ["address_saver.js"]
	},
	"browser_action":{
		"default_icon": "manutd.png",
		"default_popup": "body.html"
	}
}
